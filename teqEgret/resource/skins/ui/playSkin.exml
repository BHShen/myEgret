<?xml version="1.0" encoding="utf-8"?>
<e:Skin class="playSkin" width="1334" height="750" xmlns:e="http://ns.egret.com/eui" xmlns:w="http://ns.egret.com/wing" xmlns:tween="egret.tween.*">
	<w:Declarations>
		<tween:TweenGroup id="_btnTween">
			<tween:TweenItem target="{_btn_play}">
				<tween:Set>
					<tween:props>
						<e:Object scaleX="{1}" scaleY="{1}"/>
					</tween:props>
				</tween:Set>
				<tween:To duration="1500">
					<tween:props>
						<e:Object scaleX="{0.8}" scaleY="{0.8}"/>
					</tween:props>
				</tween:To>
				<tween:To duration="1500">
					<tween:props>
						<e:Object scaleX="{1}" scaleY="{1}"/>
					</tween:props>
				</tween:To>
				<tween:To duration="150">
					<tween:props>
						<e:Object scaleX="{1}" scaleY="{1}"/>
					</tween:props>
				</tween:To>
			</tween:TweenItem>
		</tween:TweenGroup>
		<tween:TweenGroup id="balloonTween">
			<tween:TweenItem target="{_img_balloon1}">
				<tween:Set/>
				<tween:To duration="1000">
					<tween:props>
						<e:Object rotation="{-3}"/>
					</tween:props>
				</tween:To>
				<tween:To duration="1000">
					<tween:props>
						<e:Object rotation="{0}"/>
					</tween:props>
				</tween:To>
				<tween:To duration="1000">
					<tween:props>
						<e:Object rotation="{3}"/>
					</tween:props>
				</tween:To>
				<tween:To duration="1000">
					<tween:props>
						<e:Object rotation="{0}"/>
					</tween:props>
				</tween:To>
			</tween:TweenItem>
			<tween:TweenItem target="{_img_balloon2}">
				<tween:Set/>
				<tween:To duration="1000">
				<tween:props>
					<e:Object rotation="{-3}"/>
				</tween:props>
				</tween:To>
				<tween:To duration="1000">
				<tween:props>
					<e:Object rotation="{0}"/>
				</tween:props>
				</tween:To>
				<tween:To duration="1000">
				<tween:props>
					<e:Object rotation="{3}"/>
				</tween:props>
				</tween:To>
				<tween:To duration="1000">
					<tween:props>
						<e:Object rotation="{0}"/>
