	<e:Image id="_img_balloon2" source="teq_json.img_balloon_3" x="404.5" y="136" anchorOffsetX="31.5" anchorOffsetY="93"/>
	<e:Image id="_img_balloon3" source="teq_json.img_balloon_2" x="1108.5" y="403" anchorOffsetX="201.5" anchorOffsetY="403"/>
	<e:Image id="_btn_play" source="teq_json.btn_play" x="658" y="591.5" anchorOffsetX="99" anchorOffsetY="89.5"/>
	<e:Group id="_group_play" width="200" height="200" x="562" y="496"/>
</e:Skin>