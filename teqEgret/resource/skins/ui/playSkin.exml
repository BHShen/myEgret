				</tween:To>
			</tween:TweenItem>
			<tween:TweenItem target="{_img_balloon3}">
				<tween:Set/>
				<tween:To duration="1000">
				<tween:props>
					<e:Object rotation="{-3}"/>
				</tween:props>
				</tween:To>
				<tween:To duration="1000">
				<tween:props>
					<e:Object rotation="{0}"/>
				</tween:props>
				</tween:To>
				<tween:To duration="1000">
				<tween:props>
					<e:Object rotation="{3}"/>
				</tween:props>
				</tween:To>
				<tween:To duration="1000">
					<tween:props>
						<e:Object rotation="{0}"/>
					</tween:props>
				</tween:To>
			</tween:TweenItem>
		</tween:TweenGroup>
		
	</w:Declarations>
	<e:Image source="bg_loadingbg_png" x="0" y="0"/>
